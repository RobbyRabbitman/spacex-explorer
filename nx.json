    },
    "shared-data-data-common": {
      "tags": ["scope:shared", "type:data"]
    },
    "shared-types-common": {
      "tags": ["scope:shared", "type:types"]
    },
    "shared-types-launch": {
      "tags": ["scope:shared", "type:types"]
    },
    "shared-types-rocket": {
      "tags": ["scope:shared", "type:types"]
    },
    "shared-util-util-ts": {
      "tags": ["scope:shared", "type:util"]
    },
    "web-launch-timeline": {
      "tags": ["scope:web", "type:app"]
    },
    "web-launch-timeline-e2e": {
      "tags": [],
      "implicitDependencies": ["web-launch-timeline"]
