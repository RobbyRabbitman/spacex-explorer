{
  "projectType": "library",
  "sourceRoot": "libs/launch-timeline/feature-lazy-launch-timeline/src",
  "prefix": "launch-timeline",
  "targets": {
    "build": {
      "executor": "@nrwl/angular:ng-packagr-lite",
      "outputs": ["dist/libs/launch-timeline/feature-lazy-launch-timeline"],
      "options": {
        "project": "libs/launch-timeline/feature-lazy-launch-timeline/ng-package.json",
        "tailwindConfig": "libs/launch-timeline/feature-lazy-launch-timeline/tailwind.config.js"
      },
      "configurations": {
        "production": {
          "tsConfig": "libs/launch-timeline/feature-lazy-launch-timeline/tsconfig.lib.prod.json"
        },
        "development": {
          "tsConfig": "libs/launch-timeline/feature-lazy-launch-timeline/tsconfig.lib.json"
        }
      },
      "defaultConfiguration": "production"
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/launch-timeline/feature-lazy-launch-timeline"],
      "options": {
        "jestConfig": "libs/launch-timeline/feature-lazy-launch-timeline/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "libs/launch-timeline/feature-lazy-launch-timeline/**/*.ts",
          "libs/launch-timeline/feature-lazy-launch-timeline/**/*.html"
        ]
      }
    }
  },
  "tags": ["scope:shared", "type:feature-lazy"]
}
